{
    "compilerOptions": {
      "outDir": "./dist",
      "module": "commonjs",
      "target": "es2020",
      "lib": ["esnext", "webworker"],
      "alwaysStrict": true,
      "strict": true,
      "preserveConstEnums": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "types": [
        "@cloudflare/workers-types",
        "@types/jest",
        "@types/service-worker-mock"
      ]
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist", "test"]
  }